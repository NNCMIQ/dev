ELSEIF [Number of Costing Objects in roll]=1 and [Row-1:Number of Costing Objects in roll]=3
THEN [Row-1:YTD Share]*[Row-1:YTD unit cost roll up]*[Row-1:Yield] + [Row-2:YTD Share]*[Row-2:YTD unit cost roll up]*[Row-2:Yield] + [Row-3:YTD Share]*[Row-3:YTD unit cost roll up]*[Row-3:Yield] + [YTD Unit cost]

/* 2 lines to 2 lines */ 
ELSEIF [Number of Costing Objects in roll]=2 and 
(
([Line Number]=1 AND [Row-1:Number of Costing Objects in roll]=2)
or
([Line Number]=2 AND [Row-2:Number of Costing Objects in roll]=2)
)
THEN [Row-2:YTD unit cost roll up]*[Row-2:Yield] + [YTD Unit cost]

/* 3 lines to 3 lines */ 
ELSEIF [Number of Costing Objects in roll]=3 and 
(
([Line Number]=1 AND [Row-1:Number of Costing Objects in roll]=3)
or
([Line Number]=2 AND [Row-2:Number of Costing Objects in roll]=3)
or
([Line Number]=3 AND [Row-3:Number of Costing Objects in roll]=3)
)
THEN [Row-3:YTD unit cost roll up]*[Row-3:Yield] + [YTD Unit cost]

/* 1 lines to 2 line */ 
ELSEIF [Number of Costing Objects in roll]=2 and 
(
([Line Number]=1 AND [Row-1:Number of Costing Objects in roll]=1)
or
([Line Number]=2 AND [Row-2:Number of Costing Objects in roll]=1)
)
THEN 
         (
         IF [Line Number]=1 
         THEN [Row-1:YTD unit cost roll up]*[Row-1:Yield] + [YTD Unit cost]
         ELSEIF [Line Number]=2
