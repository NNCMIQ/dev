        <Node ToolID="633">
          <GuiSettings Plugin="AlteryxBasePluginsGui.Formula.Formula">
            <Position x="3632" y="1194" />
          </GuiSettings>
          <Properties>
            <Configuration>
              <FormulaFields>
                <FormulaField expression="IF [YTD Cost Center Machine Hours] != 0 &#xA;THEN [RunTot_Cost Base (Cost Center, ParCost, Cost Element) ]/[YTD Cost Center Machine Hours] &#xA;ELSE Null()&#xA;ENDIF&#xA;&#xA;/* The price of running the line (or step on the line) for 1hr */" field="YTD Activity price machine" size="8" type="Double" />
                <FormulaField expression="IF [YTD Costing Object Machine Hours] != 0 AND  [YTD Volume]!= 0 &#xA;THEN [YTD Costing Object Machine Hours]/[YTD Volume]&#xA;ELSE 0&#xA;ENDIF&#xA;&#xA;/*Machine factor explains how many units we can produce per hour, or how much we can produce on individual steps. &#xA;In order to capture an optimization on the line (or optimizations done for production of individual step), we have to recalculate YTD machine factor.&#xA;&gt;&gt; YTD Costing Object Machine Hours means total hours spent to run the step on the line (to produce onematerial) */" field="YTD Machine factor" size="8" type="Double" />
                <FormulaField expression="IF [YTD Cost Center Machine Hours] != 0 &#xA;THEN [YTD Activity price machine]*[YTD Machine factor]&#xA;ELSE 0&#xA;ENDIF&#xA;&#xA;/*A step unit cost uses price for running the line (step on the line) per hour multiplied by how much we can produce on this line&#xA;YTD Cost Center Machine Hours stands for cost centers total hours (hr spend to run the line/or step on the line, if several products on the line)*/" field="YTD Unit cost" size="8" type="Double" />
                <FormulaField expression="[YTD Unit cost]*[YTD Volume]" field="YTD Cost Base" size="8" type="Double" />
